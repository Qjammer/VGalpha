	$(CC) $(CFLAGS) $(Managers_DIR)ManagerInterface.cpp $(FFLAGS)

$(ODIR)TaskManager.o: $(Managers_DIR)TaskManager.cpp $(Managers_DIR)TaskManager.hpp
	$(CC) $(CFLAGS) $(Managers_DIR)TaskManager.cpp $(FFLAGS)

ManagersPH:$(ODIR)Manager.o $(ODIR)ManagerInterface.o $(ODIR)TaskManager.o ThreadPoolPH

