	$(CC) $(CFLAGS) $(Managers_DIR)TaskManager.cpp $(FFLAGS)

$(ODIR)TaskManagerInterface.o: $(Managers_DIR)TaskManagerInterface.cpp $(Managers_DIR)TaskManagerInterface.hpp
	$(CC) $(CFLAGS) $(Managers_DIR)TaskManagerInterface.cpp $(FFLAGS)

$(ODIR)EnvironmentManager.o: $(Managers_DIR)EnvironmentManager.cpp $(Managers_DIR)EnvironmentManager.hpp
	$(CC) $(CFLAGS) $(Managers_DIR)EnvironmentManager.cpp $(FFLAGS)

$(ODIR)EnvironmentManagerInterface.o: $(Managers_DIR)EnvironmentManagerInterface.cpp $(Managers_DIR)EnvironmentManagerInterface.hpp
	$(CC) $(CFLAGS) $(Managers_DIR)EnvironmentManager.cpp $(FFLAGS)

