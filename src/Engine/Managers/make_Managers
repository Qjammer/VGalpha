Managers_DIR = ./Engine/Managers/

$(ODIR)Manager.o: $(Managers_DIR)Manager.cpp $(Managers_DIR)Manager.hpp
	$(CC) $(CFLAGS) $(Managers_DIR)Manager.cpp $(FFLAGS)

$(ODIR)TaskManager.o: $(Managers_DIR)TaskManager.cpp $(Managers_DIR)TaskManager.hpp
	$(CC) $(CFLAGS) $(Managers_DIR)TaskManager.cpp $(FFLAGS)

$(ODIR)EnvironmentManager.o: $(Managers_DIR)EnvironmentManager.cpp $(Managers_DIR)EnvironmentManager.hpp
	$(CC) $(CFLAGS) $(Managers_DIR)EnvironmentManager.cpp $(FFLAGS)

$(ODIR)StateManager.o: $(Managers_DIR)StateManager.cpp $(Managers_DIR)StateManager.hpp
	$(CC) $(CFLAGS) $(Managers_DIR)StateManager.cpp $(FFLAGS)

$(ODIR)TaskManagerInterface.o: $(Managers_DIR)TaskManagerInterface.cpp $(Managers_DIR)TaskManagerInterface.hpp
	$(CC) $(CFLAGS) $(Managers_DIR)TaskManagerInterface.cpp $(FFLAGS)

$(ODIR)EnvironmentManagerInterface.o: $(Managers_DIR)EnvironmentManagerInterface.cpp $(Managers_DIR)EnvironmentManagerInterface.hpp
	$(CC) $(CFLAGS) $(Managers_DIR)EnvironmentManagerInterface.cpp $(FFLAGS)

ManagersPH:$(ODIR)Manager.o $(ODIR)TaskManager.o $(ODIR)EnvironmentManager.o $(ODIR)StateManager.o $(ODIR)TaskManagerInterface.o  $(ODIR)EnvironmentManagerInterface.o
