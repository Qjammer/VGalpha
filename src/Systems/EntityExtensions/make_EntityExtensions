EntitityExtensions_DIR = ./Systems/EntityExtensions/

$(ODIR)ObjectExtension.o: $(EntitityExtensions_DIR)ObjectExtension.cpp $(EntitityExtensions_DIR)ObjectExtension.hpp
	$(CC) $(CFLAGS) $(EntitityExtensions_DIR)ObjectExtension.cpp $(FFLAGS)

$(ODIR)SceneExtension.o: $(EntitityExtensions_DIR)SceneExtension.cpp $(EntitityExtensions_DIR)SceneExtension.hpp
	$(CC) $(CFLAGS) $(EntitityExtensions_DIR)SceneExtension.cpp $(FFLAGS)

$(ODIR)GraphicObjectExtension.o: $(EntitityExtensions_DIR)GraphicObjectExtension.cpp $(EntitityExtensions_DIR)GraphicObjectExtension.hpp $(EntitityExtensions_DIR)ObjectExtension.hpp
	$(CC) $(CFLAGS) $(EntitityExtensions_DIR)GraphicObjectExtension.cpp $(FFLAGS)

EntityExtensionsPH: $(ODIR)ObjectExtension.o $(ODIR)SceneExtension.o $(ODIR)GraphicObjectExtension.o
